{
    "name": "phlexible/indexer-media-bundle",
    "description": "phlexible media indexer bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["indexer", "search"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerMediaBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/indexer-bundle": "^1.2.0",
        "phlexible/queue-bundle": "^1.3",
        "phlexible/formatter": "dev-master",
        "phlexible/volume": "dev-master",
        "symfony/config": "^2.8",
        "symfony/console": "^2.8",
        "symfony/dependency-injection": "^2.8",
        "symfony/http-kernel": "^2.8",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
