{
    "name": "phlexible/indexer-media-bundle",
    "description": "phlexible media indexer bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["indexer", "search"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerMediaBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/indexer-bundle": "~1.2",
        "phlexible/queue-bundle": "~1.2",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
